<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import serpents_echelles.pages.historique_parties.composants.MonComposant?>

<VBox xmlns:fx="http://javafx.com/fxml" alignment="center"
    fx:controller="serpents_echelles.pages.historique_parties.vues.VueHistorique" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS" fx:id="VBoxPrincipal">
   <children>
      <HBox alignment="CENTER" prefHeight="48.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
         <children>
            <Text fx:id="titreHistorique" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Historique des parties" textAlignment="CENTER" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <ScrollPane fx:id="ScrollPaneHistorique" VBox.vgrow="NEVER">
         <content>
            <VBox fx:id="VBoxHistorique" prefHeight="211.0" />
         </content>
      </ScrollPane>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS" />
      <Button fx:id="btnFermer" alignment="CENTER" mnemonicParsing="false" onMouseEntered="#changeColor" onMouseExited="#backColor" style="-fx-background-color: black;" text="Fermer" textFill="WHITE" VBox.vgrow="ALWAYS">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</VBox>